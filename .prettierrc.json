{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "trailingComma": "es5",
  "tabWidth": 4,
  "semi": true,
  "singleQuote": true,
  "experimentalTernaries": true,
  "printWidth": 100,
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "importOrder": ["^components/(.*)$", "~/components/(.*)$", "^composables/(.*)$", "~/composables/(.*)$", "^node_modules/(.*)$", "^[./]", "~/stores/(.*)$"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
